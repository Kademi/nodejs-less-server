{
    "javascript.preferences.quoteStyle": "single",
    "js/ts.implicitProjectConfig.target": "ESNext",
    "jshint.enable": true,
    "typescript.format.enable": false,
    "typescript.suggest.enabled": false,
    "typescript.validate.enable": false,
    "jshint.lintHTML": true,
    "jshint.options": {
        "curly": true,
        "esversion": 9,
        "nonbsp": true,
        "unused": "vars",
        "undef": true,
        "evil": true,
        "laxbreak": true
    },
    "yaml.schemas": {
        "https://json.schemastore.org/github-workflow.json": "${workspaceFolder}/.github/workflows/deploy.yml"
    }
}